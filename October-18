import numpy as np

x = np.array([[4, 8, 1],
             [2, 7, 9], 
             [3, 4, 8]])
x_inverse = np.linalg.inv(x)

print('Original Matrix', x)
print('==================')
print('Inverse Matrix', x_inverse)


